php_flag display_errors on
php_value error_reporting 2847

RewriteEngine on

Option -Indexes
ErrorDocument 404 /404

<IfModule mod_php.c>
    php_flag session.use_trans_sid off
</IfModule>

<IfModule mod_php8.c>
    php_flag session.use_trans_sid off
</IfModule>

<IfModule mod_rewrite.c>
    Options +FollowSymLinks
    RewriteEngine On

    RewriteCond %{REQUEST_FILENAME} !/urlrewrite.php$
    RewriteRule ^(.*)$ /urlrewrite.php [L]
</IfModule>

<IfModule mod_dir.c>
    DirectoryIndex / index.php index.html
</IfModule>

<IfModule mod_expires.c>
    ExpiresActive on
    ExpiresType image/jpeg "access plus 3 day"
    ExpiresType image/gif "access plus 3 day"
    ExpiresType image/png "access plus 3 day"
    ExpiresType text/css "access plus 3 day"
    ExpiresType application/javascript "access plus 3 day"
</IfModule>